		    <OMV name="G"/>
		    <OMV name="G"/>
		  </OMA>
		</OMA>
	      </OMATP>
	      <OMV name="F"/>
	    </OMATTR>
	  </OMBVAR>
	  <OMA>
	    <OMS cd="logic1" name="and"/>
	    <OMA>
	      <OMS cd="deussen.chap3" name="left-ideal"/>
	      <OMV name="T"/>
	      <OMV name="G"/>
	      <OMV name="F"/>
	    </OMA>
	    <OMBIND>
	      <OMS cd="quant1" name="forall"/>
	      <OMBVAR>
		<OMATTR>
		  <OMATP>
		    <OMS cd="simpletypes" name="type"/>
		    <OMA>
		      <OMS cd="simpletypes" name="funtype"/>
		      <OMV name="A"/>
