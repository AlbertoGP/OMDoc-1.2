  <symbol name="left-ideal" role="object" scope="global">
    <metadata><dc:subject  xml:lang="de">Linksideal $T$ von $G$</dc:subject>
<dc:subject> left ideal</dc:subject></metadata>
  </symbol>


  <definition xml:id="left-ideal.def" for="#left-ideal" type="simple">
    <CMP xml:lang="de">
      <legacy format="TeX"> 
	Eine nichtleere Teilmenge $T \subset F$ einer Halbgruppe $F$ hei"st  
	Linksideal [ Rechtsideal\/], wenn $FT \subset T$ [$TF \subset T$\/] gilt. 
	Ist $T$ gleichzeitig Links- und Rechtsideal, so hei"st $T$ ein  zweiseitiges Ideal. 
      </legacy>
    </CMP>
      <OMOBJ xmlns="http://www.openmath.org/OpenMath">  
	<OMBIND>
	  <OMS cd="fns1" name="lambda"/>
	  <OMBVAR>
	    <OMATTR>
	      <OMATP>
		<OMS cd="simpletypes" name="type"/>
		<OMA>
		  <OMS cd="simpletypes" name="funtype"/>
		  <OMV name="A"/>
		  <OMS cd="truthval" name="bool"/>
		</OMA>
	      </OMATP>
	      <OMV name="T"/>
	    </OMATTR>
	    <OMATTR>
	      <OMATP>
		<OMS cd="simpletypes" name="type"/>
		<OMA>
		  <OMS cd="simpletypes" name="funtype"/>
		  <OMV name="A"/>
		  <OMS cd="truthval" name="bool"/>
		</OMA>
	      </OMATP>
	      <OMV name="G"/>
	    </OMATTR>
	    <OMATTR>
	      <OMATP>
		<OMS cd="simpletypes" name="type"/> 
		<OMA>
		  <OMS cd="simpletypes" name="funtype"/>
		  <OMV name="G"/>
		  <OMA>
		    <OMS cd="simpletypes" name="funtype"/>
		    <OMV name="G"/>
		    <OMV name="G"/>
		  </OMA>
		</OMA>
	      </OMATP>
	      <OMV name="F"/>
	    </OMATTR>
	  </OMBVAR>
	  <OMA>
	    <OMS cd="logic1" name="and"/>
	    <OMA>
	      <OMS cd="logic1" name="and"/>
	      <OMA>
		<OMS cd="logic1" name="and"/>
		<OMA>
		  <OMS cd="basic.algebra" name="subset"/>
		  <OMV name="T"/>
		  <OMV name="G"/>
		</OMA>
		<OMA>
		  <OMS cd="deussen.chap1" name="semi-group"/>
		  <OMV name="G"/>
		  <OMV name="F"/>
		</OMA>
	      </OMA>
	      <OMA>
		<OMS cd="basic.algebra" name="notequal"/>
		<OMS cd="basic.algebra" name="empty-set"/>
		<OMV name="T"/>
	      </OMA>
	    </OMA>
	    <OMA>
	      <OMS cd="basic.algebra" name="subset"/>
	      <OMA>
		<OMS cd="deussen.chap1" name="complex-product"/>
		<OMV name="G"/>
		<OMV name="G"/>
		<OMV name="T"/>
		<OMV name="F"/>
	      </OMA>
	      <OMV name="T"/>
	    </OMA>
	  </OMA>
	</OMBIND>
      </OMOBJ>
  </definition>

