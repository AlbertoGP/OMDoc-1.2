<!-- the content definition from Module RT -->
<!ENTITY % omdocrt.ul.qname "%omdoc.pfx;ul">
<!ENTITY % omdocrt.ol.qname "%omdoc.pfx;ol">
<!ENTITY % omdocrt.p.qname "%omdoc.pfx;p">
<!ENTITY % omdocrt.highlight.qname "%omdoc.pfx;highlight">
<!ENTITY % omdocrt.table.qname "%omdoc.pfx;table">
<!ENTITY % omdocrt.note.qname "%omdoc.pfx;note">
<!ENTITY % omdocrt.link.qname "%omdoc.pfx;link">
<!ENTITY % omdocrt.mtext.mix "|%omdocrt.ul.qname;
                              |%omdocrt.ol.qname;		     
                              |%omdocrt.p.qname;		     
                              |%omdocrt.highlight.qname;
                              |%omdocrt.note.qname;
                              |%omdocrt.link.qname;
                              |%omdocrt.table.qname;">
	
